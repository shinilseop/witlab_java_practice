package JAVA_01_first;

import java.util.Scanner;

public class Main {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);

		int gcd = 1;
		int lcm=1;
		boolean isRun = true;
		boolean prime[] = new boolean[max + 1];
		int len = sc.nextInt();
		int arr[] = new int[len];
		int max = 0;

		System.out.print("최소공배수, 최대공약수를 계산할 숫자의 갯수 : ");
		for (int i = 0; i < len; i++) {
			System.out.print((i + 1) + "번째 숫자 입력 : ");
			arr[i] = sc.nextInt();
			max = Math.max(max, arr[i]);
		}

		for (int i = 2; i <= max; i++) {
			if (!prime[i]) {
				for (int j = i + i; j <= max; j += i) {
					prime[j] = true;
				}
			}
		}
		
		while (isRun) {
			for (int i = 2; i < prime.length; i++) {
				if (!prime[i]) {
					for (int j = 0; j < arr.length; j++) {
						if(arr[j]%i!=0)
							break;
						if(j==arr.length-1){
							divide(arr, j);
							i--;
							gcd*=j;
						}
					}
				}
			}
		}
	}
	
	public static void divide(int arr[], int div) {
		for(int i=0;i<arr.length;i++) {
			arr[i]/=div;
		}
	}
}
