package Exam02;

import javax.swing.JLabel;
import javax.swing.JTextArea;

public class FillWater {

	Water w;
	UI ui;
	
	FillWater(Water w, UI ui){
		this.w=w;
		this.ui=ui;
	}
	
	public void fillWater() {
		w.setWater(w.getWater()+100);
		jtaFill.append("100ml 부음\n");
		jlWater.setText(w.getWater()+"");
	}
}
