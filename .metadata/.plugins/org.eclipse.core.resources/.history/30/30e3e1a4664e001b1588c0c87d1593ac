package JAVA_02_last;

import java.util.Random;

import javax.swing.JLabel;
import javax.swing.JPanel;

public class MoneyGenerator extends Thread {
	JPanel panel;
	MoneyGenerator(JPanel panel){
		this.panel=panel;
	}
	public void run() {
		Random r=new Random();
		while(true) {
			Money m = new Money(r.nextInt(405)+5, 50);
			JLabel jlM=new JLabel("money.png");
			jlM.setBounds(r.nextInt(405)+5, 50, 30, 30);
			panel.add(jlM);
		}
	}
}
