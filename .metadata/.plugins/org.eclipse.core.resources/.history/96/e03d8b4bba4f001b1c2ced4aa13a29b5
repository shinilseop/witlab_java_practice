package Exam02;

import javax.swing.JTextArea;

public class FillWater {

	Water w;
	JTextArea jtaFill;
	
	FillWater(Water w, JTextArea jtaFill){
		this.w=w;
		this.jtaFill=jtaFill;
	}
	
	public voud fillWater() {
		w.setWater(w.getWater()+100);
		jtaSink.append("100ml 빠짐, "+w.getWater()+"ml 남음\n");
	}
}
