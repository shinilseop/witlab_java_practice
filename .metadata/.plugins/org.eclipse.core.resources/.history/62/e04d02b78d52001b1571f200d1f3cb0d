		super(uic);
		
		
		
		
		isRun=true;
		gc=new Tetris();
	}
	
	
	public void run() {
		while(isRun) {
			
		}
	}

	@Override
	public void keyPressed(KeyEvent e) {
		// TODO Auto-generated method stub
		int key=e.getKeyCode();
		if(key==KeyEvent.VK_UP) {
			
		} else if(key==KeyEvent.VK_LEFT) {
			
		}  else if(key==KeyEvent.VK_LEFT) {
			
		}  else if(key==KeyEvent.VK_LEFT) {
			
		}
	}
}
